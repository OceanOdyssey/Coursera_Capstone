    "# create map\n",
    "map_clusters = folium.Map()\n",
    "\n",
    "for lat, lon, address, city in zip(df_all_houses['LATITUDE'], df_all_houses['LONGITUDE'], df_all_houses['ADDRESS'], df_all_houses['CITY']):\n",
    "    folium.CircleMarker(\n",
    "        [lat, lon],\n",
    "        radius=5,\n",
    "        tooltip = ('Address: ' + str(address) + '<br>'\n",
    "                 'City: ' + str(city) + '<br>'\n",
    "                 ),\n",
    "        color='blue',\n",
    "        fill_color='blue',\n",
    "        fill_opacity=.7\n",
    "        ).add_to(map_clusters)\n",
    "    \n",
    "for lat, lon, address, city in zip(df_residence['LATITUDE'], df_residence['LONGITUDE'], df_residence['ADDRESS'], df_residence['CITY']):\n",
    "    folium.CircleMarker(\n",
    "        [lat, lon],\n",
    "        radius=5,\n",
    "        tooltip = ('Address: ' + str(address) + '<br>'\n",
    "                 'City: ' + str(city) + '<br>'\n",
    "                 ),\n",
    "        color='red',\n",
    "        fill_color='red',\n",
    "        fill_opacity=.7\n",
    "        ).add_to(map_clusters)\n",
    "    \n",
    "#Set the zoom to the fit data\n",
    "map_clusters.fit_bounds(map_clusters.get_bounds())\n",
    "print('Hover over circles to display information about location.  Blue Circle = House for Sale, Red = Current Residence')\n",
    "map_clusters"
