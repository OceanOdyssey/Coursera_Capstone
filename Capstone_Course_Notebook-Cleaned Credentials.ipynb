    "Function `return_amenities` to return the nearby amenities for each house which fit defined requirements."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 291,
   "metadata": {},
   "outputs": [],
   "source": [
    "def return_amenities(addresses, cities, latitudes, longitudes, categories, distances):\n",
    "    search_results=[]\n",
    "    for category_id,distance in zip(categories, distances):    \n",
    "        print('Searching for Category ID :',category_id) \n",
    "        for address, city, lat, lng in zip(addresses, cities, latitudes, longitudes):\n",
    "            \n",
    "            # create the API request URL\n",
    "            url = 'https://api.foursquare.com/v2/venues/explore?&client_id={}&client_secret={}&v={}&ll={},{}&radius={}&categoryId={}&limit={}'.format(\n",
    "                CLIENT_ID, \n",
    "                CLIENT_SECRET, \n",
    "                VERSION, \n",
    "                lat, \n",
    "                lng, \n",
    "                distance,\n",
    "                category_id,\n",
    "                LIMIT\n",
    "                )\n",
    "            # Make the GET request.  If request fails there are two \"except\" options to handle the error.\n",
    "            try:\n",
    "                results = requests.get(url).json()\n",
    "                venue_name = results['response']['groups'][0]['items'][0]['venue']['name']\n",
    "                lat = results['response']['groups'][0]['items'][0]['venue']['location']['lat']\n",
    "                lng = results['response']['groups'][0]['items'][0]['venue']['location']['lng']\n",
    "                dist = round(meters_to_miles(results['response']['groups'][0]['items'][0]['venue']['location']['distance']),1)\n",
    "                cat = results['response']['groups'][0]['items'][0]['venue']['categories'][0]['name']\n",
    "                search_results.append([address, city, venue_name, lat, lng, dist, cat, category_id])  #append data to search_results\n",
    "            #If there is no venue found\n",
    "            except IndexError:\n",
    "                venue_name = None\n",
    "                lat = None\n",
    "                lng = None\n",
    "                dist = None\n",
    "                cat = None\n",
    "                search_results.append([address, city, venue_name, lat, lng, dist, cat, category_id])  #append data to search_results\n",
    "            #If there is another type of error, most likely \"quota exceeded\"\n",
    "            except: \n",
    "                print('Error: ',requests.get(url).json())         \n",
    "\n",
    "    return search_results"
   ]
