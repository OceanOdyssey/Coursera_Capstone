    "# create map\n",
    "\n",
    "map_clusters = folium.Map()\n",
    "\n",
    "for lat, lon, Type, Venue_Name in zip(single_house_venues['Latitude'], single_house_venues['Longitude'], single_house_venues['Type'], single_house_venues['Venue_Name']):\n",
    "    folium.CircleMarker(\n",
    "        [lat, lon],\n",
    "        radius=5,\n",
    "        tooltip = ('Venue Type: ' + str(Type) + '<br>'\n",
    "                 'Venue Name: ' + str(Venue_Name)\n",
    "                 ),\n",
    "        color='blue',\n",
    "        fill_color='blue',\n",
    "        fill_opacity=.7\n",
    "        ).add_to(map_clusters)\n",
    "    \n",
    "for lat, lon, address, city in zip(single_house['LATITUDE'], single_house['LONGITUDE'], single_house['ADDRESS'], single_house['CITY']):\n",
    "    folium.CircleMarker(\n",
    "        [lat, lon],\n",
    "        radius=5,\n",
    "        tooltip = ('House for Sale' '<br>'\n",
    "                   'Address: ' + str(address) + '<br>'\n",
    "                 'City: ' + str(city) + '<br>'\n",
    "                 ),\n",
    "        color='orange',\n",
    "        fill_color='orange',\n",
    "        fill_opacity=.7\n",
    "        ).add_to(map_clusters)\n",
    "    \n",
    "#Set the zoom to the maximum possible\n",
    "map_clusters.fit_bounds(map_clusters.get_bounds())\n",
    "print('Hover over circles to display information about location.  Orange circle = House for Sale, Blue = Required Venue')\n",
    "map_clusters"
